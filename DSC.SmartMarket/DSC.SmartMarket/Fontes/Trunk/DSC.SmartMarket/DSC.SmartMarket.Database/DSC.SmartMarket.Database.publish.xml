<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>SmartMarket</TargetDatabaseName>
    <DeployScriptFileName>DSC.SmartMarket.Database.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=digitalsmartcrafters.database.windows.net;Persist Security Info=True;User ID=administrador;Pooling=False;MultipleActiveResultSets=False;Connect Timeout=60;Encrypt=False;TrustServerCertificate=True</TargetConnectionString>
